{
    "id": "jrpg-ui-module",
    "title": "JRPG UI Module by Niqu3d",
    "description": "A custom JRPG-style UI for FoundryVTT (PF2e) with character sheets inspired by Legend of Dragoon.",
    "url": "https://github.com/Niqu3d/jrpg-ui-module",
    "version": "0.1.7",
    "authors": [
        {
            "name": "Niqu3d",
            "url": "https://github.com/Niqu3d/jrpg-ui-module/",
            "flags": {}
        }
    ],
    "relationships": {
        "systems": [
            {
                "id": "pf2e",
                "type": "system",
                "compatibility": {
                    "minimum": "6.8.0",
                    "verified": "6.8.0"
                }
            }
        ]
    },
    "esmodules": [
        "scripts/index.js",
        "scripts/characterSheet.js",
        "scripts/party-screen.js",
        "scripts/extendActor.js",
        "scripts/controlButton.js"
    ],
    "styles": [
        "styles/style.css",
        "styles/party-screen.css"
    ],
    "files": [
        "**fonts/**"
    ],
    "sheets": {
        "jrpg-ui-module.LoDCharacterSheet": {
            "class": "modules/jrpg-ui-module/scripts/characterSheet.js",
            "type": "actor"
        }
    },
    "compatibility": {
        "minimum": "12",
        "verified": "12"
    },
    "manifest": "https://github.com/Niqu3d/jrpg-ui-module/releases/latest/download/module.json",
    "download": "https://github.com/Niqu3d/jrpg-ui-module/releases/latest/download/jrpg-ui-module.zip",
    "readme": "https://github.com/Niqu3d/jrpg-ui-module"
}
